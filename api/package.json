{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "rm -rf dist; tsc",
    "start": "node dist",
    "test": "yarn test:unit; yarn test:integration; yarn test:e2e",
    "test:unit": "node_modules/.bin/mocha -r ts-node/register src/**/*.unit.spec.ts -r dotenv/config --require babel-register",
    "test:integration": "env-cmd node_modules/.bin/mocha -r ts-node/register src/**/*.integration.spec.ts --require babel-register",
    "test:e2e": "node_modules/.bin/mocha -r ts-node/register src/**/*.e2e.spec.ts --require babel-register",
    "docker:create:dev": "docker run -d --name mydria_mongo_dev -e MONGODB_USERNAME=mydria_dev -e MONGODB_PASSWORD=mydria_dev -e MONGODB_DATABASE=mydria_db_dev -p 27020:27017 bitnami/mongodb:latest",
    "docker:remove:dev": "docker stop mydria_mongo_dev; docker rm mydria_mongo_dev",
    "docker:create:test": "docker run -d --name mydria_mongo_test -e MONGODB_USERNAME=mydria_test -e MONGODB_PASSWORD=mydria_test -e MONGODB_DATABASE=mydria_db_test -p 27020:27017 bitnami/mongodb:latest",
    "docker:remove:test": "docker stop mydria_mongo_test; docker rm mydria_mongo_test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/murilo9/mydria-2.git"
  },
  "keywords": [],
  "author": "Murilo Henrique Matias",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/murilo9/mydria-2/issues"
  },
  "homepage": "https://github.com/murilo9/mydria-2#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^4.1.1",
    "node-mailjet": "^3.3.4",
    "typescript": "^4.4.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.13",
    "@types/node-mailjet": "^3.3.7",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.3.4",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "mocha": "^9.1.1",
    "sinon": "^11.1.2",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8"
  }
}